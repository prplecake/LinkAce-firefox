{
  "env": {
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions":{
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "chrome": false
  },
  "rules":{
    "semi": 2,
    "quotes": ["warn", "single"],
    "@typescript-eslint/no-non-null-assertion": "off",
    "max-len": ["error", 80]
  }
}